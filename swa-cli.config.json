{
  "configurations": {
    "blazor.youtube-downloader": {
      "appLocation": "Blazor.YouTubeDownloader",
      "apiLocation": "Blazor.YouTubeDownloader.Api",
      "outputLocation": "bin/wwwroot",
      "apiLanguage": "dotnetisolated",
      "apiVersion": "8.0",
      "appBuildCommand": "dotnet publish -c Release -o bin",
      "apiBuildCommand": "dotnet publish -c Release",
      "run": "dotnet watch run",
      "appDevserverUrl": "http://localhost:8000"
    }
  }
}